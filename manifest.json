{
  "name": "WebSentry Chrome Plugin",
  "description": "This extension runs web checking code.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": [
        "section/checkheader.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "popup.css",
        "popup.js",
        "bootstrap.min.css",
        "bootstrap.bundle.min.js",
        "checkheader.js"
      ],
      "matches": ["<all_urls>"]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}